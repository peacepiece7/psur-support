    "@rive-app/canvas": "^2.33.3",
    "@vee-validate/nuxt": "^4.13.2",
    "@vee-validate/yup": "^4.15.1",
    "@vuepic/vue-datepicker": "^12.1.0",
    "@vueuse/core": "^11.3.0",
    "axios": "^1.13.2",
    "ckeditor5": "^43.2.0",
    "pinia": "^2.2.2",
    "uuid": "^13.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.5",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.10.6",
    "@types/yup": "^0.32.0",
    "@vueuse/nuxt": "^12.4.0",
    "cross-env": "^7.0.3",
    "dayjs-nuxt": "^2.1.11",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "fontagon": "^1.1.1",
    "nuxt": "^3.15.1",
    "openapi-typescript-codegen": "^0.30.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rimraf": "^6.1.2",
    "sass": "^1.83.0",
    "sass-embedded": "^1.77.8",
    "sass-loader": "^16.0.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.3",
    "vite-plugin-vuetify": "^2.1.2",
    "vue3-carousel-nuxt": "^1.1.6",
    "vuetify": "^3.11.2"
  },
  "pnpm": {
    "overrides": {
      "nuxi": "3.8.4"
