{
  "name": "psur-front",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://pg-nuxt-vuetify-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-nuxt-vuetify-tailwindcss.git"
  },
  "scripts": {
    "dev": "nuxt dev --dotenv .env.local",
    "now": "npm run build && npm run preview",
    "now:dev": "cross-env NITRO_PRESET=node-server npx nuxi build && npx nuxi preview",
    "gen:icon": "node scripts/iconfont.cjs",
    "gen:oas": "node scripts/generate-open-api-sepc.cjs",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-nuxt-vuetify-tailwindcss.netlify.app",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxt analyze",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write \"**/*.{html,vue,svelte,astro,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
